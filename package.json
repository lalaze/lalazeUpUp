{
  "name": "lalazeupup",
  "version": "1.0.0",
  "description": "练习帮助构建工具建的库，以后有用也可以直接上",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "eslint src --max-warnings 0",
    "build": "npm run lint && rimraf dist types && gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lalaze/lalazeUpUp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lalaze/lalazeUpUp/issues"
  },
  "homepage": "https://github.com/lalaze/lalazeUpUp#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "conventional-changelog": "^3.1.21",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.6.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "install": "^0.13.0",
    "merge2": "^1.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "files": [
    "dist",
    "types"
  ]
}
